big, cite, code, em, font, img, small, strike, sub,
sup, li, ol, ul, fieldset, form, label, legend, button,
table, caption, tr, th, td {
	border: 0;
	font-family: inherit;
	font-size: 100%;
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	margin: 0;
	padding: 0;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

ol,
ul {
	list-style: none;
}

