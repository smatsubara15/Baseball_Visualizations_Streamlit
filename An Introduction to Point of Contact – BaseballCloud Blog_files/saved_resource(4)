/**
 * 15. Media Queries
 * ----------------------------------------------------------------------------
 *
 * Styles to get the theme behaving responsively on smaller screens
 */


@media (max-width: 1440px) {
	.wrapper.section {
		padding: 5% 0;
	}

	/* pagination */

	.page-title {
		margin-bottom: 5%;
	}

	.posts-navigation {
		margin-top: 1%;
	}
}


